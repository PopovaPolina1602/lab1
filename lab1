#include <stdio.h>

int main(){
    printf("Требуется ввести число - ");
    
    int n; 
    int l = 1; 
    int m = 0, sum = 0;
    
    scanf("%d", &n);
    
    while (sum < n){
        sum = sum + l;
        l = l + 2;
        m++;
    }
    
    printf("Квадратный корень равен %d\n", m);
    return 0;
}
